    "@tanstack/react-query": "^5.72.0",
    "axios": "^1.8.4",
    "framer-motion": "^12.6.3",
    "html2canvas": "^1.4.1",
    "json-server": "^1.0.0-beta.3",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.5.25",
    "lucide-react": "^0.487.0",
    "miragejs": "^0.1.48",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-redux": "^9.2.0",
